{
  "name": "@qlever-llc/target-helper",
  "version": "2.3.3",
  "description": "A Trellis microservice to fill in around Target integration",
  "author": "Andrew Balmos <andrew@qlever.io>",
  "license": "Apache-2.0",
  "repository": "https://github.com/Qlever-LLC/target-helper",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "ava": {
    "failFast": false,
    "files": ["**/*.test.ts"],
    "typescript": {
      "extensions": ["ts"],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": ["text", "lcov"],
    "all": true,
    "src": "lib",
    "exclude": ["*.d.ts", ".pnp.*", ".test"]
  },
  "dependencies": {
    "@oada/client": "^5.3.0",
    "@oada/jobs": "^4.7.4",
    "@oada/lib-prom": "^4.0.1",
    "@oada/list-lib": "^6.1.0",
    "@oada/pino-debug": "^4.0.1",
    "@oada/types": "^3.5.3",
    "@overleaf/o-error": "^3.4.0",
    "@trellisfw/signatures": "^2.1.3",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "ksuid": "^3.0.0",
    "make-promises-safe": "^5.1.0",
    "moment": "^2.30.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@biomejs/biome": "^1.9.4",
    "@tsconfig/node22": "^22.0.1",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.14",
    "@yarnpkg/sdks": "^3.2.1",
    "assign-deep": "^1.0.1",
    "ava": "6.2.0",
    "isomorphic-timers-promises": "^1.0.1",
    "json-ptr": "^3.1.1",
    "lodash-es": "^4.17.21",
    "object-hash": "^3.0.0",
    "typescript": "5.8.2"
  },
  "resolutions": {
    "@oada/lib-prom": "^4.0.1",
    "cross-spawn": "^7.0.5",
    "jsonpath-plus": "^10.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "packageManager": "yarn@4.8.1",
  "volta": {
    "node": "22.9.0"
  }
}
