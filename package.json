{
  "name": "@qlever-llc/target-helper",
  "version": "1.6.6",
  "description": "A Trellis microservice to fill in around Target integration",
  "author": "Andrew Balmos <andrew@qlever.io>",
  "license": "Apache-2.0",
  "repository": "https://github.com/Qlever-LLC/target-helper",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "lib",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^5.1.0",
    "@oada/jobs": "^4.5.2",
    "@oada/lib-prom": "^3.8.0",
    "@oada/list-lib": "^6.0.2",
    "@oada/pino-debug": "^3.10.0",
    "@oada/types": "^3.5.3",
    "@overleaf/o-error": "^3.4.0",
    "@trellisfw/signatures": "^2.1.3",
    "axios": "^1.7.2",
    "clone-deep": "^4.0.1",
    "convict": "^6.2.4",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "ksuid": "^3.0.0",
    "make-promises-safe": "^5.1.0",
    "moment": "^2.30.1",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/clone-deep": "^4.0.4",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@yarnpkg/sdks": "^3.1.3",
    "assign-deep": "^1.0.1",
    "ava": "6.1.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "eslint-plugin-array-func": "^5.0.1",
    "eslint-plugin-ava": "^15.0.1",
    "eslint-plugin-escompat": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^5.0.1",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^1.0.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^1.0.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "isomorphic-timers-promises": "^1.0.1",
    "json-ptr": "^3.1.1",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "prettier": "^3.3.2",
    "typescript": "5.5.2"
  },
  "resolutions": {
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
  },
  "packageManager": "yarn@4.3.1",
  "volta": {
    "node": "20.13.1"
  }
}
