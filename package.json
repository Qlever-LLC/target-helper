{
  "name": "@qlever-llc/target-helper",
  "version": "1.5.2",
  "description": "A Trellis microservice to fill in around Target integration",
  "author": "Andrew Balmos <andrew@qlever.io>",
  "license": "Apache-2.0",
  "repository": "https://github.com/Qlever-LLC/target-helper",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc --watch",
    "start": "node ${INSPECT:-} --enable-source-maps dist/index.js",
    "test": "yarn build test && ava",
    "test:debug": "ava -T 60m -svc 1 --no-worker-threads"
  },
  "ava": {
    "failFast": false,
    "files": [
      "**/*.test.ts"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "src/": "dist/",
        "test/": ".test/"
      },
      "compile": false
    }
  },
  "c8": {
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true,
    "src": "lib",
    "exclude": [
      "*.d.ts",
      ".pnp.*",
      ".test"
    ]
  },
  "dependencies": {
    "@oada/client": "^4.3.2",
    "@oada/jobs": "^4.1.6",
    "@oada/list-lib": "^4.2.2",
    "@oada/types": "^3.2.1",
    "@overleaf/o-error": "^3.4.0",
    "@trellisfw/signatures": "^2.1.3",
    "axios": "^0.27.2",
    "clone-deep": "^4.0.1",
    "convict": "^6.2.3",
    "convict-format-with-moment": "^6.2.0",
    "convict-format-with-validator": "^6.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "ksuid": "^3.0.0",
    "make-promises-safe": "^5.1.0",
    "moment": "^2.29.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/clone-deep": "^4.0.1",
    "@types/convict": "^6.1.1",
    "@types/convict-format-with-validator": "^6.0.2",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@yarnpkg/sdks": "^3.0.0-rc.20",
    "assign-deep": "^1.0.1",
    "ava": "4.3.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-regexp": "^1.9.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "isomorphic-timers-promises": "^1.0.1",
    "json-ptr": "^3.1.1",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  },
  "packageManager": "yarn@3.2.3"
}
